#!/bin/bash

name=${1}

if [[ "${taito_provider:-}" == "ssh" ]] && \
   [[ "${ci_exec_deploy:-}" == "true" ]] && \
   taito::confirm_execution "aws" "${name}" \
   "Configure SSH credentials for CI/CD pipeline"
then
  echo
  echo "TODO: Instructions"
  echo
  echo "Press enter when done."
  read -r
fi

taito::call_next "${@}"
