#!/bin/bash
: "${taito_plugin_path:?}"
: "${taito_project:?}"
: "${taito_env:?}"
: "${taito_namespace:?}"

name=${1}

if taito::confirm_execution "docker-delete-secrets" "${name}" \
  "Delete secrets of from ./secrets/${taito_env}"
then
  "${taito_plugin_path}/util/delete-secrets"
fi && \

taito::call_next "${@}"
